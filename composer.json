{
    "name": "iggyvolz/phlum",
    "description": "Simple PHP framework",
    "type": "library",
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "phan/phan": "^4.0",
        "squizlabs/php_codesniffer": "^3.6",
        "vimeo/psalm": "^4.3",
        "phpstan/phpstan": "^0.12.80",
        "jetbrains/phpstorm-attributes": "^1.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Katie Volz",
            "email": "iggyvolz@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "nette/php-generator": "^3.6.1",
        "nette/utils": "^3.2.3",
        "iggyvolz/classgen": "^0.1",
        "iggyvolz/simple-attribute-reflection": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "iggyvolz\\phlum\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "iggyvolz\\phlum\\test\\": "test/"
        }
    }
}
