{
    "name": "iggyvolz/phlum",
    "description": "Simple PHP framework",
    "type": "library",
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "phan/phan": "^4.0",
        "squizlabs/php_codesniffer": "^3.6",
        "vimeo/psalm": "^4.3",
        "phpstan/phpstan": "^0.12.80",
        "jetbrains/phpstorm-attributes": "^1.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Katie Volz",
            "email": "iggyvolz@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "nette/php-generator": "dev-master#7c91b22d7d8d54ceea5f7c30300563d40ff6c2a4",
        "nette/utils": "dev-master#f06376c65d5ba63a8ab9e769f55f2bcbe722d42e",
        "iggyvolz/classgen": "dev-main",
        "iggyvolz/simple-attribute-reflection": "dev-main"
    },
    "autoload": {
        "psr-4": {
            "iggyvolz\\phlum\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "iggyvolz\\phlum\\test\\": "test/"
        }
    }
}
