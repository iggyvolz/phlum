{
    "name": "iggyvolz/phlum",
    "description": "Simple PHP framework",
    "type": "library",
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "phan/phan": "^4.0",
        "squizlabs/php_codesniffer": "^3.6",
        "vimeo/psalm": "^4.3",
        "phpstan/phpstan": "^0.12.80",
        "jetbrains/phpstorm-attributes": "^1.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Katie Volz",
            "email": "iggyvolz@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "nette/php-generator": "dev-master#5fe11779a644dce6410205acfa1c85bd088b260f",
        "nette/utils": "dev-master#dc67af69b6586a2ff086d5c0fbaccada67e9486d",
        "iggyvolz/classgen": "dev-main",
        "iggyvolz/simple-attribute-reflection": "dev-main"
    },
    "autoload": {
        "psr-4": {
            "iggyvolz\\phlum\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "iggyvolz\\phlum\\test\\": "test/"
        }
    },
    "repositories": [{
        "type": "git",
        "url": "https://github.com/iggyvolz/php-generator.git"
    }]
}
