{
    "name": "iggyvolz/phlum",
    "description": "Simple PHP framework",
    "type": "library",
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "phan/phan": "^4.0",
        "squizlabs/php_codesniffer": "^3.6",
        "vimeo/psalm": "^4.3",
        "phpstan/phpstan": "^0.12.80",
        "jetbrains/phpstorm-attributes": "^1.0",
        "guzzlehttp/guzzle": "^7.4"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Katie Volz",
            "email": "iggyvolz@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "nette/php-generator": "^v3.6.4",
        "nette/utils": "^3.2.5",
        "iggyvolz/classgen": "dev-main",
        "iggyvolz/simple-attribute-reflection": "dev-main",
        "psr/http-client": "^1.0",
        "nyholm/psr7": "^1.4"
    },
    "autoload": {
        "psr-4": {
            "iggyvolz\\phlum\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "iggyvolz\\phlum\\test\\": "test/"
        }
    }
}
